{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "http://localhost:4321/"
      }
    }
  ]
}
